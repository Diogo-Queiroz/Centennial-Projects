@model IEnumerable<string>

<div class="bg-danger">
	This part uses a IEnumerable
	<ul>
		@foreach (string str in Model)
		{
			<li>@str</li>
		}
	</ul>
</div>

<div class="bg-info">
	<div>This is the message from the Partial View</div>
	<a asp-action="Index">This is a link to the Index action</a>
</div>
